   "source": [
    "import turtle\n",
    "t=turtle.Turtle()\n",
    "#Vẽ hình tròn có bán kính bằng 100 và độ rộng bằng 5\n",
    "m=100\n",
    "t.pensize(5)\n",
    "t.pencolor('red')\n",
    "t.fillcolor('blue')\n",
    "t.begin_fill()\n",
    "t.circle(m)\n",
    "t.end_fill()\n",
    "turtle.done()\n"
   ]
